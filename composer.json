{
    "name": "pandawa/arjuna",
    "description": "Distributed event component for pandawa",
    "keywords": [
        "laravel",
        "pandawa",
        "arjuna",
        "microservices",
        "event"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "type": "library",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "pandawa/foundation-bundle": "^5.0",
        "pandawa/event-bundle": "^5.0",
        "pandawa/console-bundle": "^5.0",
        "pandawa/serializer-bundle": "^5.0",
        "pandawa/transformer": "^5.0",
        "pandawa/dependency-injection-bundle": "^5.0",
        "pandawa/annotation-bundle": "^5.0",
        "pandawa/dependency-injection-annotations": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Arjuna\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Arjuna\\ArjunaBundle"
            ]
        }
    }
}
